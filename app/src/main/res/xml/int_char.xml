        <Key android:keyIcon="@drawable/sym_keyboard_delete"  android:keyWidth="21%p" android:keyEdgeFlags="right"
            android:isRepeatable="true" android:codes="-5"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-222" android:keyLabel="ABC" android:keyWidth="11%p" android:keyEdgeFlags="left"
            android:isRepeatable="true"/>
        <Key android:keyLabel=":)" android:keyWidth="10%p"  android:codes="-9"/>
