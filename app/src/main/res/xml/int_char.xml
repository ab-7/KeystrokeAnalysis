    <Row>
        <Key android:keyIcon="@drawable/symbol_1"  android:keyWidth="10%p" android:keyEdgeFlags="left"
            android:isRepeatable="true" android:codes="-111"/>
        <Key android:keyLabel="\!"  android:codes="33"/>
        <Key android:keyLabel="\#" android:codes="35"/>
        <Key android:keyLabel="\=" android:codes="61"/>
        <Key android:keyLabel='"' android:codes="34"/>
