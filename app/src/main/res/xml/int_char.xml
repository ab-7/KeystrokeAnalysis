            android:codes="56"/>
        <Key android:keyLabel="9"
            android:codes="57"/>
        <Key android:keyLabel="0"

            android:codes="48"/>
    </Row>

    <Row>
        <Key android:keyLabel="\@" android:keyEdgeFlags="left" android:codes="64"/>
        <Key android:keyLabel="\^" android:codes="94"/>
        <Key android:keyLabel="\&amp;" android:codes="38"/>
        <Key android:keyLabel="\_" android:codes="95"/>
